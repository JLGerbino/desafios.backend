<div>
    <h1>Cargar producto</h1>
        <label>Titulo</label>
        <input id = "title" name="title"><br>

        <label>Descripcion</label>
        <input id = "description" name="description"><br>

        <label>Codigo</label>
        <input id = "code" name="code"><br>

        <label>Precio</label>
        <input id = "price" name="price" type="number"><br>

        <label>Stock</label>
        <input id = "stock" name="stock" type="number"><br>

        <label>Categoria</label>
        <input id = "category" name="category"><br>

        <label>Imagenes</label>
        <input id = "thumbnail" name="thumbnail"><br> 
        {{!-- de aca saque el type="file" --}}

        <input id="enviar" type="submit">   
   
</div>
<div>
<h1>Eliminar producto</h1>
<label>ingresar Id</label>
    <input id = "id" name="id" type="text" size="30"><br>
<input id="eliminar" type="submit">
</div>
<h1>Listado de productos en tiempo real</h1>

<div id="inicio">
    {{#each products}}
      <p id="idn">id: {{this._id}}, title: {{this.title}}, description: {{this.description}}, code: {{this.code}}, price: {{this.price}}, status: {{this.status}}, stock: {{this.stock}}, category: {{this.category}}, thumbnail: {{this.thumbnail}}</p><br>
    {{/each}}
</div>

<p id="productos"></p>

<script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>
<script src="/socket.io/socket.io.js"></script>
<script src="/js/home.js"></script>